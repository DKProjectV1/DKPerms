<?xml version="1.0" encoding="UTF-8"?>
<project name="module_permissionsystem-bukkit" default="compile.module.permissionsystem-bukkit">
  <dirname property="module.permissionsystem-bukkit.basedir" file="${ant.file.module_permissionsystem-bukkit}"/>
  
  <property name="module.jdk.home.permissionsystem-bukkit" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.permissionsystem-bukkit" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.permissionsystem-bukkit" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.permissionsystem-bukkit" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="permissionsystem-bukkit.output.dir" value="${module.permissionsystem-bukkit.basedir}/target/classes"/>
  <property name="permissionsystem-bukkit.testoutput.dir" value="${module.permissionsystem-bukkit.basedir}/target/test-classes"/>
  
  <path id="permissionsystem-bukkit.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="permissionsystem-bukkit.module.production.classpath">
    <path refid="${module.jdk.classpath.permissionsystem-bukkit}"/>
    <fileset dir="${basedir}/../dependency/spigot">
      <patternset refid="library.patterns"/>
    </fileset>
    <fileset dir="${basedir}/../dependency/bukkit">
      <patternset refid="library.patterns"/>
    </fileset>
    <pathelement location="${permissionsystem-lib.output.dir}"/>
    <path refid="library.maven:_org.mongodb:mongo-java-driver:3.8.0.classpath"/>
    <path refid="library.maven:_org.spigotmc:spigot-api:1.8.8-r0.1-snapshot.classpath"/>
